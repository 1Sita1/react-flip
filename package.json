{
    "name": "react-flip",
    "version": "1.0.0",
    "description": "React UI library for flipping cards",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "test": "test",
        "build": "rmdir dist /s /q && prettier --write src/**/*.ts && npm run build:esm && npm run build:cjs",
        "build:esm": "tsc",
        "build:cjs": "tsc --module CommonJS --outDir dist/cjs"
    },
    "keywords": [
        "flip",
        "card",
        "react"
    ],
    "author": "Arsenii Alkhimenko",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^18.7.14",
        "@types/react": "^18.0.18",
        "@types/react-dom": "^18.0.6",
        "typescript": "^4.8.2"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "dependencies": {
        "prettier": "^2.7.1",
        "react-jss": "^10.9.2"
    }
}
